{
  "name": "maa-common",
  "type": "module",
  "scripts": {
    "build": "cd packages/cli && npm run build",
    "dev": "cd packages/cli && npm run dev"
  },
  "bin": "dist/scripts/index.js",
  "pkg": {
    "scripts": "dist/scripts/index.js",
    "assets": "dist/build/**/*",
    "targets": [
      "node18-macos-x64",
      "node18-win-x64",
      "node18-linux-x64"
    ],
    "outputPath": "dist"
  },
  "workspaces": [
    "packages/*"
  ]
}
